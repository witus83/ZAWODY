SELECT GROUP_CONCAT(Name) FROM TYPE T
JOIN COMPETITION_TYPE CT ON CT.TYPE_ID=T.ID
JOIN COMPETITION C ON C.ID=CT.COMPETITION_ID
WHERE C.Date BETWEEN '2016-05-01' AND '2016-05-10'



select DISTINCT
C.ID,C.COMPETITION_NAME, C.Date,
(SELECT GROUP_CONCAT(Name) FROM TYPE) AS TYPE
FROM COMPETITION C 
LEFT JOIN COMPETITION_TYPE CT ON C.ID=CT.COMPETITION_ID
LEFT JOIN TYPE T ON T.ID=CT.TYPE_ID
WHERE C.Date BETWEEN '2016-05-01' AND '2016-05-10'



select DISTINCT
C.ID
FROM COMPETITION C 
LEFT JOIN COMPETITION_TYPE CT ON C.ID=CT.COMPETITION_ID
LEFT JOIN TYPE T ON T.ID=CT.TYPE_ID
WHERE C.Date BETWEEN '2016-05-01' AND '2016-05-10'




CREATE TEMPORARY TABLE COMP (
ID INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
COMPETITION_NAME VARCHAR(300),
TYPES VARCHAR (300),
DATE DATE
)
